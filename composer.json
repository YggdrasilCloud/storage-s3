{
    "name": "yggdrasilcloud/storage-s3",
    "description": "Amazon S3 storage adapter bridge for YggdrasilCloud",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "aws/aws-sdk-php": "^3.333",
        "symfony/http-foundation": "^7.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.89",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.4",
        "symfony/phpunit-bridge": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "YggdrasilCloud\\StorageS3\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YggdrasilCloud\\StorageS3\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
